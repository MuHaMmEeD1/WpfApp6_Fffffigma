<Page x:Class="WpfApp6_Figma.Pages.Landing"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WpfApp6_Figma.Pages"
      mc:Ignorable="d" 
       d:DesignHeight="700" d:DesignWidth="375" ShowsNavigationUI="False"
      Title="Landing" Background="#f0f0f0">

    <Grid>

        <Grid.Resources>

            <ControlTemplate TargetType="Button" x:Key="imageBorder">

                <Border
            
                    CornerRadius="25"
                    Height="50"
                    Width="50"
                    
                    
                    >

                    <Border.Background>
                        <ImageBrush ImageSource="../Images/default.png" />
                    </Border.Background>


                </Border>

            </ControlTemplate>

            <Style TargetType="Border" x:Key="Border">

                <Setter Property="Height" Value="34"/>
                <Setter Property="Width" Value="335"/>
                <Setter Property="CornerRadius" Value="3"/>
                <Setter Property="Background" Value="#FFFFFF"/>



            </Style>

            <Style TargetType="Border" x:Key="BigBorder">

                <Setter Property="Height" Value="146"/>
                <Setter Property="Width" Value="335"/>
                <Setter Property="CornerRadius" Value="3"/>
                <Setter Property="Background" Value="#FFFFFF"/>

            </Style>


            <Style TargetType="Border" x:Key="MiniBigBorder">

                <Setter Property="Height" Value="48"/>
                <Setter Property="Width" Value="335"/>
                <Setter Property="CornerRadius" Value="3"/>
                <Setter Property="Background" Value="#FFFFFF"/>

            </Style>

            <Style TargetType="Border" x:Key="MiniMiniBorder">

                <Setter Property="Height" Value="50"/>
                <Setter Property="Width" Value="50"/>
                <Setter Property="CornerRadius" Value="3"/>
                <Setter Property="Background" Value="#FFFFFF"/>

            </Style>


        </Grid.Resources>
      

        <Label 
               
               Template="{DynamicResource PassKeeperImage}" 
               
               HorizontalAlignment="Left"
               VerticalAlignment="Top"
               Margin="123 29 0 0" Grid.ColumnSpan="2"
               />

        <Button 
            
            Height="50"
            Width="50"
            
            Template="{DynamicResource imageBorder}"
            VerticalAlignment="Top"
            HorizontalAlignment="Left"
            Margin="20 20 0 0"
            />

        <Border 
            
            Style="{DynamicResource Border}"
            VerticalAlignment="Top"
            Margin="20,90,20,0" Grid.ColumnSpan="2"
            
            />

        <TextBox 
            
            Height="34"
            Width="315"
            VerticalAlignment="Top"
            Margin="30,90,30,0"
            BorderThickness="0"
            
            Foreground="#C0C0C0"
            FontSize="18"
            FontWeight="DemiBold"
            FontFamily="Corbel"
            
            VerticalContentAlignment="Center"
            
            Text="search" Grid.ColumnSpan="2"
            
            
            />

        <Image 
